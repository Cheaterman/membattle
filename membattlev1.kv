<MyGridS@Widget>
    AnchorLayout:
        size: root.width,root.height
        padding: [30,20,30,60]
        anchor_y: "top"
        anchor_x: "center"
        GridLayout:
            cols: 1
            spacing: 5
            LogInBLable:
                text: "Sign up form"
                font_size: 22
                size_hint: 1,0.5
                color: 0,0,0,1
            GridLayout:
                spacing: 3
                cols:2
                rows:4
                LogInBLable:
                    font_size: 12
                    text: "Username:"
                    color: 0,0,0,1
                    bold: True
                TextInput:
                    id: user_2
                    padding_y: [self.height / 2.0 - (self.line_height / 2.0) * len(self._lines), 0]
                    font_size: 14
                    multiline:False
                LogInBLable:
                    font_size: 12
                    text: "Password:"
                    color: 0,0,0,1
                    bold: True
                TextInput:
                    id: pass_2
                    padding_y: [self.height / 2.0 - (self.line_height / 2.0) * len(self._lines), 0]
                    font_size: 14
                    multiline:False
            EditedButton1:
                font_size: 30
                size_hint: 0.9,0.9
                text: "Submit"
                color: 0.2,0.9,0.9,1
                bold: True
                on_press:
                    self.back_color=(0,0,0,1)
                    root.DBsend(user_2, pass_2)
                    root.clear_inputs([user_2, pass_2])
                on_release:
#                    app.root.current = "main"
#                    app.root.transition.direction = 'left'
                    self.back_color=(0.2,0.9,0.9,1)
            GridLayout:
                size_hint: 1,0.2
                cols: 1
                Button:
                    background_color: 0,0,0,0
                    color: 0.2,0.4,0.6,1
                    font_size: 10
                    text: 'Already signed up?'
                    on_press:
                        self.color=(0,0,0,1)
                    on_release:
                        self.color=(0.2,0.4,0.6,1)
                        app.root.current= "third"
                        app.root.transition.direction = 'left'

    AnchorLayout:
        anchor_y: "bottom"
        anchor_x: "left"
        EditedButton:
            size_hint: .65,.22
            text:"Back"
            bold: True
            on_press: self.back_color=(0,0,0,1)
            on_release:
                app.root.current = "main"
                app.root.transition.direction = 'right'
                self.back_color=(0.2,0.9,0.9,1)

